<Window x:Class="Updater.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Updater"
        mc:Ignorable="d"
        Title="Aktualisierung" Height="116" Width="446" ResizeMode="NoResize" Icon="logo.jpg">
    <Grid>
        <Label x:Name="label" Content="Update wird installiert. Dies kann ein moment dauern." HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <ProgressBar x:Name="progress" HorizontalAlignment="Left" Height="30" Margin="10,36,0,0" VerticalAlignment="Top" Width="408"/>
    </Grid>
</Window>
